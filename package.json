{
  "name": "vue3-google-map",
  "version": "0.1.0",
  "license": "MIT",
  "description": "",
  "unpkg": "dist/cjs/index.min.js",
  "jsdelivr": "dist/cjs/index.min.js",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "autoindex": "yarn do autoindex --sfc",
    "clear": "rimraf dist/**/*",
    "build": "run-s clear autoindex build:lib build:types",
    "build:lib": "node ./build/build cjs es --min",
    "build:types": "echo - Building types && ttsc -P tsconfig.es.json --outDir dist/types --declaration",
    "docs": "npx vitepress dev ./docs",
    "docs:build": "npx vitepress build ./docs"
  },
  "dependencies": {
    "connect-google-maps": "0.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/google-maps": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "common-types": "^1.15.8",
    "cross-env": "^7.0.2",
    "do-devops": "^0.5.4",
    "eslint": "^7.10.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.3",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "ttypescript": "^1.5.12",
    "typescript": "~4.0.3",
    "typescript-transform-paths": "^2.0.1",
    "vite": "^1.0.0-rc.4",
    "vitepress": "^0.6.0",
    "vue": "3"
  },
  "peerDependencies": {
    "vue": "3"
  },
  "engines": {
    "node": ">=10"
  }
